<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="hu.petrik.adatbazis.DogController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TableView fx:id="dogtable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="idcol" minWidth="50.0" prefWidth="200.0" text="Id" />
            <TableColumn fx:id="namecol" minWidth="50.0" prefWidth="200.0" text="Name" />
          <TableColumn fx:id="agecol" minWidth="50.0" prefWidth="200.0" text="Age" />
          <TableColumn fx:id="readcol" minWidth="50.0" prefWidth="200.0" text="Breed" />
        </columns>
      </TableView>
   </children>
</VBox>
